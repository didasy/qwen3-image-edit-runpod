# Hugging Face Token (optional, for private models or higher rate limits)
HF_TOKEN=hf_your_huggingface_token_here

# Hugging Face Cache Directory (optional)
HF_HOME=/data/huggingface

# Enable hf-transfer for faster model downloads
HF_HUB_ENABLE_HF_TRANSFER=1

# Backblaze B2 (S3-compatible) Configuration
S3_ENDPOINT=s3.us-west-000.backblazeb2.com
S3_REGION=us-west-000
S3_ACCESS_KEY=your_backblaze_key_id
S3_SECRET_KEY=your_backblaze_application_key
S3_BUCKET=your_bucket_name
S3_SECURE=true
S3_OBJECT_PREFIX=qwen-image-edit/

# Presigned URL Expiry (in seconds)
PRESIGN_EXPIRY=86400

# Maximum Image Size (in bytes) - 25MB default
MAX_IMAGE_BYTES=26214400

# Timeout for the entire operation (in seconds)
TIMEOUT_SECONDS=120

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO